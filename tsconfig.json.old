{
   "compilerOptions": {
      "typeRoots": ["./@types", "./node_modules/@types"],
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictNullChecks": true,
      "sourceMap": true
   }
}